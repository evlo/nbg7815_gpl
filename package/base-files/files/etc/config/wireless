config qcawifi 'qcawifi'
	option atf_mode '1'

config iface iface
	option wifi24G    'wifi1' #ax 2.4G
	option wifi5G     'wifi0' #ax 5G

config WiFi wifi
	option on '1'
	option ApproveFlag '1'
	option guest_vid '200'

config wifi-device 'wifi1'
	option type 'qcawificfg80211'
	option channel_ext '2'
	option auth 'OPEN'
	option rts '2346'
	option frag '2346'
	option AutoChannelSelect '1'
	option txpower '100'
	option disabled '0'
	option channel_width '20'
	option hwmode '11axg'
	option dbdc_enable '0'
	option log_level '1'
	option country 'US'
	option atfstrictsched 0

config wifi-iface 'ath0'
	option device 'wifi1'
	option network 'lan'
	option mode 'ap'
	option wpa_group_rekey '3600'
	option PreAuth '0'
	option hidden '0'
	option wmm '1'
	option encryption 'psk2'
	option ap_bridge '1'
	option rts '2346'
	option frag '2346'
	option disabled '1'
	option shortgi '1'
	option AP_autoconfig '1'
	option ieee80211w '0'
	option wds '1'
	option type 'main'
	option rrm '1'
	option athnewind '1'
	option nrshareflag '3'
	option ngvhtintop '1'
	option wnm '1'
	option commitatf '1'
	option wps_pbc '1'
	option atfssidgroup '1'

config wifi-iface 'ath1'
	option device 'wifi1'
	option network 'guest'
	option mode 'ap'
	option wpa_group_rekey '3600'
	option PreAuth '0'
	option hidden '0'
	option wmm '1'
	option encryption 'psk2'
	option ap_bridge '0'
	option disabled '1'
	option shortgi '1'
	option enable_guest_wlan '1'
	option guest_ip '192.168.141.1'
	option guest_ip_mask '255.255.255.0'
	option guest_bandwidth_enable '0'
	option guest_max_bandwidth '256'
	option ieee80211w '0'
	option wds '1'
	option type 'guest'
	option rrm '1'
	option athnewind '1'
	option nrshareflag '3'
	option wnm '1'
	option commitatf '1'

